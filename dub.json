{
    "name": "dcollectors",
    "version": "0.0.1",
    "description": "Collectors for OpenTSDB/tcollector",
    "copyright": "Copyright Â© 2015, oozie",
    "authors": ["oozie"],
    "platform": "linux",
    "buildRequirements": [
        "requireBoundsCheck", "disallowDeprecations"
    ],

    "sourcePaths": ["source/acpi_temp"],
    "targetType": "executable",
    "targetName": "example",

    "subPackages": [
        {
            "name": "acpi_temp",
            "sourcePaths": ["source/acpi_temp"],
            "targetName": "acpi_temp",
            "targetPath": "collectors/0",
        },
        {
            "name": "logscanner",
            "sourcePaths": ["source/logscanner"],
            "targetName": "logscanner",
            "targetPath": "collectors/0",
        },
        {
            "name": "utils",
            "sourcePaths": ["source/utils"],
            "targetName": "collector",
            "targetPath": "collectors",
        },
    ]
}
